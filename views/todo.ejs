      <input type="text" name="item" placeholder="Add new item..." required>
      <button value="submit">Add Item</button>
    </form>
  </div>

  <ul>
    <li>Item 1</li>
    <li>Item 2</li>
    <li>Item 3</li>
    <li>Item 4</li>
  </ul>

</body>
